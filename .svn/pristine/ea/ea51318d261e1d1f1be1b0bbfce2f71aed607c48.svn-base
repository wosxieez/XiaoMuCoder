package com.xiaomu.event
{
	import flash.events.Event;
	
	/**
	 * 钩子事件
	 * @author coco
	 */	
	public class ActionHookEvent extends Event
	{
		
		public static const TARGET_CHANGED:String = "targetChanged";
		
		public static const MATCH_CHANGED:String = "matchChanged";
		
		
		public function ActionHookEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false)
		{
			super(type, bubbles, cancelable);
		}
		
	}
}