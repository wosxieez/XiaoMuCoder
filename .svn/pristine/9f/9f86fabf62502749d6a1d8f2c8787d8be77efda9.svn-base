package com.xiaomu.component
{
	
	[ExcludeClass]
	public class FlagHookComponent extends HookComponent
	{
		public function FlagHookComponent()
		{
			super();
		}
		
		override public function match(hook:HookComponent):Boolean
		{
			if (hook is FlagHookComponent)
				return super.match(hook);
			else
				return false;
		}
		
	}
}